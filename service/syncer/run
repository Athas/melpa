#!/bin/bash -e

SCRIPTPATH=$( cd "$(dirname "$0")" ; pwd -P )
MELPA_REPO="${SCRIPTPATH}/../.."
cd "${MELPA_REPO}"

[[ -s "../.rvm/scripts/rvm" ]] && source "../.rvm/scripts/rvm"

unset STABLE

make cleanup
make json
make html


export STABLE=t

make cleanup
make json
make html

# Sync every 5 minutes.
sleep 5m
